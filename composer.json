{
    "name": "geomywp/geo-my-wp",
    "type": "wordpress-plugin",
    "description": "The ultimate geolocation and mapping solution for WordPress.",
    "keywords": ["gmw", "geomywp", "GEO my WP"],
    "homepage": "https://geomywp.com/",
    "license": "GPL-3.0+",
    "support": {
        "issues": "https://geomywp.com/support"
    },
    "config": {
        "disable-tls": true,
        "secure-http": false,
        "allow-plugins": {
          "composer/installers": true
        }
    },
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
    	"wp-coding-standards/wpcs": "2.3.0",
    	"phpunit/phpunit": "4.8.* || 5.7.* || ^6.5.14",
        "phpunit/php-invoker": "1.1.3",
        "yoast/phpunit-polyfills": "1.x-dev"
    },
    "minimum-stability": "dev"
}
